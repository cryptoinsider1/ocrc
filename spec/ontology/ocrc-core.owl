<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:ocrc="https://ocrc.org/ns/v1#">

  <owl:Ontology rdf:about="https://ocrc.org/ns/v1">
    <rdfs:comment>OCRC Core Ontology v1.0</rdfs:comment>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:about="https://ocrc.org/ns/v1#ResearchObject">
    <rdfs:label>Research Object</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://ocrc.org/ns/v1#AstronomicalObject">
    <rdfs:subClassOf rdf:resource="https://ocrc.org/ns/v1#ResearchObject"/>
  </owl:Class>

  <owl:Class rdf:about="https://ocrc.org/ns/v1#Event">
    <rdfs:subClassOf rdf:resource="https://ocrc.org/ns/v1#ResearchObject"/>
  </owl:Class>

  <owl:Class rdf:about="https://ocrc.org/ns/v1#Hypothesis">
    <rdfs:subClassOf rdf:resource="https://ocrc.org/ns/v1#ResearchObject"/>
  </owl:Class>

  <owl:Class rdf:about="https://ocrc.org/ns/v1#Method">
    <rdfs:subClassOf rdf:resource="https://ocrc.org/ns/v1#ResearchObject"/>
  </owl:Class>

  <owl:Class rdf:about="https://ocrc.org/ns/v1#Agent">
    <rdfs:label>Agent (person or organization)</rdfs:label>
  </owl:Class>

  <!-- Properties -->
  <owl:DatatypeProperty rdf:about="https://ocrc.org/ns/v1#identifier">
    <rdfs:domain rdf:resource="https://ocrc.org/ns/v1#ResearchObject"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="https://ocrc.org/ns/v1#proposedBy">
    <rdfs:domain rdf:resource="https://ocrc.org/ns/v1#Hypothesis"/>
    <rdfs:range rdf:resource="https://ocrc.org/ns/v1#Agent"/>
  </owl:ObjectProperty>
</rdf:RDF>
